<link rel="stylesheet" type="text/css" href="/assets/css/allfeedback.css">

<div id="container">
    <div style="display:flex;justify-content: center;padding: 10px;width: 800px; font-weight: bold;">
        <label style="font-size: 14px;width: 18%">Emp Name</label>
        <label style="font-size: 14px;width: 18%">Teamwork</label>
        <label style="font-size: 14px;width: 18%">Communication</label>
        <label style="font-size: 14px;width: 18%">Accuracy Of Work</label>
        <label style="font-size: 14px;width: 18%">Attendance</label>
    </div>
    <div style="display:flex;justify-content: center;padding: 10px;width: 800px;">
        <label style="width: 18%">
            <%=feedback.employeeName%>
        </label>
        <form method="post" action="/feedback/updateFeedBack">
            <input type="text" name="feedbackId" value="<%=feedback._id%>" hidden/>
            <input style="width: 18%; margin-left: 5px;margin-right: 5px;" name="teamwork"
                value="<%=feedback.teamwork%>"></input>
            <input style="width: 18%; margin-left: 5px;margin-right: 5px;" name="communication"
                value="<%=feedback.communication%>"></input>
            <input style="width: 18%; margin-left: 5px;margin-right: 5px;" name="accuracyOfWork"
                value="<%=feedback.accuracyOfWork%>"></input>
            <input style="width: 18%; margin-left: 5px;margin-right: 5px;" name="attendance"
                value="<%=feedback.attendance%>"></input>
            <input id="updateDetailsButton" class="btn btn-primary" type="submit" value="Update" />
        </form>
    </div>

</div>